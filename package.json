    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "exceljs": "^4.3.0",
    "faker": "^5.5.3",
    "fastify": "^4.23.2",
    "mongodb": "^6.1.0",
