{
  "type": "module",
  "name": "election",
  "version": "1.0.0",
  "description": "project for sending text messages for pepole to vote !",
  "main": "index.js",
  "scripts": {
    "test": "vitest --silent --threads false",
    "dev": "nodemon src/index.ts",
    "lint": "eslint . --ext .ts --fix",
